<div class="page-header">
  <h1><a href="<%= root_path %>">NDC9 Fetcher <small>- A Simple API Server for NDC9</small></a></h1>
</div>

<div class="panel panel-primary">
  <div class="panel-heading">
    <h2 class="panel-title">結果</h2>
  </div>
  <div class="panel-body">
    <p>リクエストID: <%= request_id %>
    <table class="table table-striped">
      <thead>
        <tr><th>ISBN</th><th>NDC9</th></tr>
      </thead>
      <tbody>
      <% result.each do |isbn, ndc| %>
        <tr><td><%= isbn %></td><td><%= ndc %></td></tr>
      <% end %>
      </tbody>
    </table>
  </div>
</div>
