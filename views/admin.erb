<div class="page-header">
  <h1><a href="<%= root_path %>">NDC9 Fetcher <small>- A Simple API Server for NDC9</small></a></h1>
</div>

<div class="panel panel-primary" id="request_id_list">
  <div class="panel-heading">
    <h2 class="panel-title">未処理のリクエスト</h2>
  </div>
  <div class="panel-body">
    <ul>
      <% request_id_list.each do |request_id| %>
      <li><%= request_id %></li>
      <% end %>
    </ul>
  </div>
</div>

<div class="panel panel-primary" id="processing_id_list">
  <div class="panel-heading">
    <h2 class="panel-title">処理中のリクエスト</h2>
  </div>
  <div class="panel-body">
    <ul>
      <% processing_id_list.each do |request_id| %>
      <li><%= request_id %></li>
      <% end %>
    </ul>
  </div>
</div>

<div class="panel panel-primary" id="result_id_list">
  <div class="panel-heading">
    <h2 class="panel-title">処理済みのリクエスト</h2>
  </div>
  <div class="panel-body">
    <ul>
      <% result_id_list.each do |request_id| %>
      <li><a href="<%= root_path %>/v1/isbn/bulk/<%= request_id %>"><%= request_id %></li>
      <% end %>
    </ul>
  </div>
</div>
